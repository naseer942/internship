# Creating a list
my_list = [4,5,6,7,8,9]

# Adding an element
my_list.append(6)
# Removing an element
my_list.remove(7)
# Modifying an element
my_list[0] = 10

print("List:", my_list)

# Creating a dictionary
my_dict = {'name': 'Naseeruddin', 'age': 20}

# Adding a key-value pair
my_dict['city'] = 'Hyderabad'
# Removing a key-value pair
del my_dict['age']
# Modifying a value
my_dict['name'] = 'Ashu'

print("Dictionary:", my_dict)

# Creating a set
my_set = {4,5,6,7,8,9}

# Adding an element
my_set.add(3)
# Removing an element
my_set.remove(4)
# No direct modification, but you can add/remove elements

print("New Set:", my_set)
